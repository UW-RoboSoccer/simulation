<mujoco model="humanoid">
    <compiler angle="degree" inertiafromgeom="true"/>
    <default>
        <joint armature="1" damping="1" limited="true"/>
        <geom conaffinity="0" condim="3" contype="0" material="geom" rgba="0.4 0.33 0.26 1.0"/>
        <position kp="21.1" ctrlrange="-3.141592 3.141592"/>
    </default>
    <option iterations="8" timestep="0.003"/>
    <custom>
        <numeric data="2500" name="constraint_limit_stiffness"/>
        <numeric data="27000" name="constraint_stiffness"/>
        <numeric data="30" name="constraint_ang_damping"/>
        <numeric data="80" name="constraint_vel_damping"/>
        <numeric data="-0.05" name="ang_damping"/>
        <numeric data="0.5" name="joint_scale_pos"/>
        <numeric data="0.1" name="joint_scale_ang"/>
        <numeric data="0" name="spring_mass_scale"/>
        <numeric data="1" name="spring_inertia_scale"/>
        <numeric data="20" name="matrix_inv_iterations"/>
        <numeric data="15" name="solver_maxls"/>
    </custom>
    <size nkey="5" nuser_geom="1"/>
    <visual>
        <map fogend="5" fogstart="3"/>
    </visual>
    <asset>
        <texture builtin="gradient" height="100" rgb1=".4 .5 .6" rgb2="0 0 0" type="skybox" width="100"/>
        <texture builtin="flat" height="1278" mark="cross" markrgb="1 1 1" name="texgeom" random="0.01" rgb1="0.8 0.6 0.4" rgb2="0.8 0.6 0.4" type="cube" width="127"/>
        <texture builtin="checker" height="100" name="texplane" rgb1="0 0 0" rgb2="0.8 0.8 0.8" type="2d" width="100"/>
        <material name="MatPlane" reflectance="0.5" shininess="1" specular="1" texrepeat="60 60" texture="texplane"/>
        <material name="geom" texture="texgeom" texuniform="true"/>
    </asset>
    <worldbody>
        <light cutoff="100" diffuse="1 1 1" dir="-0 0 -1.3" directional="true" exponent="1" pos="0 0 1.3" specular=".1 .1 .1"/>
        <geom conaffinity="1" condim="3" friction="1 .1 .1" material="MatPlane" name="floor" pos="0 0 0" size="20 20 0.125" type="plane" rgba="0.5 0.5 0.5 1.0"/>
        <body name="torso" pos="0 0 1.4">
            <site name="IMU"/>
            <camera name="track" mode="trackcom" pos="0 -4 0" xyaxes="1 0 0 0 0 1"/>
            <joint armature="0" damping="0" limited="false" name="root" pos="0 0 0" stiffness="0" type="free"/>
            <geom contype="1" fromto="0 -.07 0 0 .07 0" name="torso1" size="0.07" type="capsule"/>
            
            <!-- Head with 2 DOF (pitch and yaw) -->
            <body name="head" pos="0 0 0.19">
                <joint name="head_pitch" type="hinge" axis="0 1 0" pos="0 0 0" range="-45 45"/>
                <joint name="head_yaw" type="hinge" axis="0 0 1" pos="0 0 0" range="-70 70"/>
                <geom contype="1" name="head" size=".09" type="sphere" user="258"/>
            </body>

            <!-- Right Arm (3 DOF at shoulder) -->
            <body name="right_upper_arm" pos="0 -0.17 0.06">
                <joint name="right_shoulder_pitch" type="hinge" axis="0 1 0" pos="0 0 0" range="-180 60"/>
                <joint name="right_shoulder_roll" type="hinge" axis="1 0 0" pos="0 0 0" range="-90 90"/>
                <joint name="right_shoulder_yaw" type="hinge" axis="0 0 1" pos="0 0 0" range="-90 90"/>
                <geom fromto="0 0 0 .16 -.16 -.16" name="right_uarm1" size="0.04 0.16" type="capsule"/>
                <body name="right_lower_arm" pos=".18 -.18 -.18">
                    <geom contype="1" fromto="0.01 0.01 0.01 .17 .17 .17" name="right_larm" size="0.031" type="capsule"/>
                    <geom contype="1" name="right_hand" pos=".18 .18 .18" size="0.04" type="sphere"/>
                </body>
            </body>

            <!-- Left Arm (3 DOF at shoulder) -->
            <body name="left_upper_arm" pos="0 0.17 0.06">
                <joint name="left_shoulder_pitch" type="hinge" axis="0 1 0" pos="0 0 0" range="-180 60"/>
                <joint name="left_shoulder_roll" type="hinge" axis="1 0 0" pos="0 0 0" range="-90 90"/>
                <joint name="left_shoulder_yaw" type="hinge" axis="0 0 1" pos="0 0 0" range="-90 90"/>
                <geom contype="1" fromto="0 0 0 .16 .16 -.16" name="left_uarm1" size="0.04 0.16" type="capsule"/>
                <body name="left_lower_arm" pos=".18 .18 -.18">
                    <geom contype="1" fromto="0.01 -0.01 0.01 .17 -.17 .17" name="left_larm" size="0.031" type="capsule"/>
                    <geom contype="1" name="left_hand" pos=".18 -.18 .18" size="0.04" type="sphere"/>
                </body>
            </body>

            <!-- Right Leg (6 DOF) -->
            <body name="right_thigh" pos="0 -0.1 -0.04">
                <!-- Hip joints -->
                <joint name="right_hip_pitch" type="hinge" axis="0 1 0" pos="0 0 0" range="-120 45"/>
                <joint name="right_hip_roll" type="hinge" axis="1 0 0" pos="0 0 0" range="-45 45"/>
                <joint name="right_hip_yaw" type="hinge" axis="0 0 1" pos="0 0 0" range="-60 60"/>
                <geom contype="1" fromto="0 0 0 0 0.01 -.34" name="right_thigh1" size="0.06" type="capsule"/>
                <body name="right_shin" pos="0 0.01 -0.403">
                    <!-- Knee joints -->
                    <joint name="right_knee_pitch" type="hinge" axis="0 1 0" pos="0 0 0" range="-160 0"/>
                    <joint name="right_knee_twist" type="hinge" axis="0 0 1" pos="0 0 0" range="-30 30"/>
                    <geom contype="1" fromto="0 0 0 0 0 -.3" name="right_shin1" size="0.049" type="capsule"/>
                    <body name="right_foot" pos="0 0 -0.45">
                        <!-- Ankle joint -->
                        <joint name="right_ankle_pitch" type="hinge" axis="0 1 0" pos="0 0 0" range="-45 45"/>
                        <geom contype="1" name="right_foot" pos="0 0 0.1" size="0.075" type="sphere" user="0"/>
                    </body>
                </body>
            </body>

            <!-- Left Leg (6 DOF) -->
            <body name="left_thigh" pos="0 0.1 -0.04">
                <!-- Hip joints -->
                <joint name="left_hip_pitch" type="hinge" axis="0 1 0" pos="0 0 0" range="-120 45"/>
                <joint name="left_hip_roll" type="hinge" axis="1 0 0" pos="0 0 0" range="-45 45"/>
                <joint name="left_hip_yaw" type="hinge" axis="0 0 1" pos="0 0 0" range="-60 60"/>
                <geom contype="1" fromto="0 0 0 0 -0.01 -.34" name="left_thigh1" size="0.06" type="capsule"/>
                <body name="left_shin" pos="0 -0.01 -0.403">
                    <!-- Knee joints -->
                    <joint name="left_knee_pitch" type="hinge" axis="0 1 0" pos="0 0 0" range="-160 0"/>
                    <joint name="left_knee_twist" type="hinge" axis="0 0 1" pos="0 0 0" range="-30 30"/>
                    <geom contype="1" fromto="0 0 0 0 0 -.3" name="left_shin1" size="0.049" type="capsule"/>
                    <body name="left_foot" pos="0 0 -0.45">
                        <!-- Ankle joint -->
                        <joint name="left_ankle_pitch" type="hinge" axis="0 1 0" pos="0 0 0" range="-45 45"/>
                        <geom contype="1" name="left_foot" type="sphere" size="0.075" pos="0 0 0.1" user="0"/>
                    </body>
                </body>
            </body>
        </body>
    </worldbody>

    <actuator>
        <!-- Head actuators -->
        <position joint="head_pitch" name="head_pitch"/>
        <position joint="head_yaw" name="head_yaw"/>
        
        <!-- Right arm actuators -->
        <position joint="right_shoulder_pitch" name="right_shoulder_pitch"/>
        <position joint="right_shoulder_roll" name="right_shoulder_roll"/>
        <position joint="right_shoulder_yaw" name="right_shoulder_yaw"/>
        
        <!-- Left arm actuators -->
        <position joint="left_shoulder_pitch" name="left_shoulder_pitch"/>
        <position joint="left_shoulder_roll" name="left_shoulder_roll"/>
        <position joint="left_shoulder_yaw" name="left_shoulder_yaw"/>
        
        <!-- Right leg actuators -->
        <position joint="right_hip_pitch" name="right_hip_pitch"/>
        <position joint="right_hip_roll" name="right_hip_roll"/>
        <position joint="right_hip_yaw" name="right_hip_yaw"/>
        <position joint="right_knee_pitch" name="right_knee_pitch"/>
        <position joint="right_knee_twist" name="right_knee_twist"/>
        <position joint="right_ankle_pitch" name="right_ankle_pitch"/>
        
        <!-- Left leg actuators -->
        <position joint="left_hip_pitch" name="left_hip_pitch"/>
        <position joint="left_hip_roll" name="left_hip_roll"/>
        <position joint="left_hip_yaw" name="left_hip_yaw"/>
        <position joint="left_knee_pitch" name="left_knee_pitch"/>
        <position joint="left_knee_twist" name="left_knee_twist"/>
        <position joint="left_ankle_pitch" name="left_ankle_pitch"/>
    </actuator>

    <sensor>
        <accelerometer name="accel" site="IMU"/>
        <gyro name="gyro" site="IMU"/>
    </sensor>

</mujoco>