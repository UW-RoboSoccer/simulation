<mujoco model="build">
    <compiler angle="degree" meshdir="assets"/>

    <option iterations="1" timestep="0.001"/>
    <custom>
        <!-- brax custom params -->
        <numeric data="2500" name="constraint_limit_stiffness"/>
        <numeric data="27000" name="constraint_stiffness"/>
        <numeric data="30" name="constraint_ang_damping"/>
        <numeric data="80" name="constraint_vel_damping"/>
        <numeric data="-0.05" name="ang_damping"/>
        <numeric data="0.5" name="joint_scale_pos"/>
        <numeric data="0.1" name="joint_scale_ang"/>
        <numeric data="0" name="spring_mass_scale"/>
        <numeric data="1" name="spring_inertia_scale"/>
        <numeric data="20" name="matrix_inv_iterations"/>
        <numeric data="15" name="solver_maxls"/>
    </custom>

    <asset>
        <material name="black" rgba="0.2 0.2 0.2 1"/>

        <texture builtin="gradient" height="100" rgb1=".4 .5 .6" rgb2="0 0 0" type="skybox" width="100"/>
        <texture builtin="checker" height="100" name="texplane" rgb1="0 0 0" rgb2="0.8 0.8 0.8" type="2d" width="100"/>
        <material name="MatPlane" reflectance="0.5" shininess="1" specular="1" texrepeat="60 60" texture="texplane"/>

        <mesh name="body" file="body.stl"/>
        <mesh file="ll1.stl"/>
        <mesh file="ll2.stl"/>
        <mesh file="ll3.stl"/>
        <mesh file="ll4.stl"/>
        <mesh file="ll5.stl"/>
        <mesh file="ll6.stl"/>
        <mesh file="rl1.stl"/>
        <mesh file="rl2.stl"/>
        <mesh file="rl3.stl"/>
        <mesh file="rl4.stl"/>
        <mesh file="rl5.stl"/>
        <mesh file="rl6.stl"/>
        <mesh name="la1" file="la1.stl"/>
        <mesh name="la2" file="la2.stl"/>
        <mesh name="la3" file="la3.stl"/>
        <mesh file="ra1.stl"/>
        <mesh file="ra2.stl"/>
        <mesh file="ra3.stl"/>
        <mesh name="h1" file="h1.stl"/>
        <mesh name="h2" file="h2.stl"/>
        <mesh name="bodyc" file="simplified_convex_decimated/body.stl"/>
        <mesh name="body1c" file="simplified_convex_decimated/body_sub1.stl"/>
        <mesh name="body2c" file="simplified_convex_decimated/body_sub2.stl"/>
        <mesh name="body3c" file="simplified_convex_decimated/body_sub3.stl"/>
        <mesh name="body4c" file="simplified_convex_decimated/body_sub4.stl"/>
        <mesh name="ll1c" file="simplified_convex_decimated/ll1.stl"/>
        <mesh name="ll2c" file="simplified_convex_decimated/ll2.stl"/>
        <mesh name="ll3c" file="simplified_convex_decimated/ll3.stl"/>
        <mesh name="ll4c" file="simplified_convex_decimated/ll4.stl"/>
        <mesh name="ll5c" file="simplified_convex_decimated/ll5.stl"/>
        <mesh name="ll6c" file="simplified_convex_decimated/ll6.stl"/>
        <mesh name="rl1c" file="simplified_convex_decimated/rl1.stl"/>
        <mesh name="rl2c" file="simplified_convex_decimated/rl2.stl"/>
        <mesh name="rl3c" file="simplified_convex_decimated/rl3.stl"/>
        <mesh name="rl4c" file="simplified_convex_decimated/rl4.stl"/>
        <mesh name="rl5c" file="simplified_convex_decimated/rl5.stl"/>
        <mesh name="rl6c" file="simplified_convex_decimated/rl6.stl"/>
        <mesh name="la1c" file="simplified_convex_decimated/la1.stl"/>
        <mesh name="la2c" file="simplified_convex_decimated/la2.stl"/>
        <mesh name="la3c" file="simplified_convex_decimated/la3.stl"/>
        <mesh name="ra1c" file="simplified_convex_decimated/ra1.stl"/>
        <mesh name="ra2c" file="simplified_convex_decimated/ra2.stl"/>
        <mesh name="ra3c" file="simplified_convex_decimated/ra3.stl"/>
        <mesh name="h1c" file="simplified_convex_decimated/h1.stl"/>
        <mesh name="h2c" file="simplified_convex_decimated/h2.stl"/>
        <mesh name="h21c" file="simplified_convex_decimated/h2_sub1.stl"/>
        <mesh name="h22c" file="simplified_convex_decimated/h2_sub2.stl"/>
    </asset>

    <default>
        <joint armature="1" damping="1" limited="true"/>
        <geom conaffinity="0" condim="3" contype="1" type='box' rgba="0.4 0.33 0.26 1.0" size="1 1 1"/>
        <mesh scale="0.01 0.01 0.01"/>
        <motor ctrllimited="true" ctrlrange="-.4 .4"/>
        <default class="visual">
            <geom material="black" contype="0" conaffinity="0" group="2"/>
        </default>
        <default class="collision">
            <geom material="black" contype="1" conaffinity="1" group="1"/>
        </default>
    </default>

    <worldbody>
        <light cutoff="100" diffuse="1 1 1" dir="-0 0 -1.3" directional="true" exponent="1" pos="0 0 1.3" specular=".1 .1 .1"/>
        <geom conaffinity="1" condim="3" friction="1 .1 .1" material="MatPlane" name="floor" pos="0 0 0" size="100 100 0.125" type="plane" rgba="0.5 0.5 0.5 1.0"/>
        <body name='body' pos='0 0 0.3'>
            <freejoint/>
            <inertial pos="0 0 1" quat="0.704708 0.704003 0.0667707 -0.0575246" mass="1.34928" diaginertia="0.00341264 0.00316574 0.00296931"/>
            <!-- <geom type="mesh" mesh="body" class="visual"/> -->
            <geom type="mesh" mesh="bodyc" class="collision"/>
            <geom type="mesh" mesh="body1c" class="collision"/>
            <geom type="mesh" mesh="body2c" class="collision"/>
            <geom type="mesh" mesh="body3c" class="collision"/>
            <geom type="mesh" mesh="body4c" class="collision"/>
            <site name="torso"/>
            <body name="head_pan_link" pos="-0.001 0 0.1365">
                <inertial pos="0.00233 0 0.00823" quat="0.663575 0.663575 0.244272 -0.244272" mass="0.01176" diaginertia="4.23401e-06 3.60599e-06 1.65e-06"/>
                <joint name="head_pan" axis="0 0 1" range="-45 45"/>
                <!-- <geom type="mesh" mesh="h1" class="visual"/> -->
                <geom type="mesh" mesh="h1c" class="collision"/>
                <body name="head_tilt_link" pos="0.01 0.019 0.0285">
                    <inertial pos="0.0023 -0.01863 0.0277" quat="0.997312 0.00973825 0.0726131 -0.00102702" mass="0.13631" diaginertia="0.000107452 8.72266e-05 4.39413e-05"/>
                    <joint name="head_tilt" axis="0 -1 0" range="-20 20"/>
                    <!-- <geom type="mesh" mesh="h2" class="visual"/> -->
                    <geom type="mesh" mesh="h2c" class="collision" contype="1"/>
                    <geom type="mesh" mesh="h21c" class="collision" contype="1"/>
                    <geom type="mesh" mesh="h22c" class="collision" contype="1"/>
                </body>
            </body>
            <body name="l_sho_pitch_link" pos="-0.001 0.06 0.111">
                <inertial pos="0 0.00823 -0.00233" quat="0.244272 0.663575 0.244272 0.663575" mass="0.01176" diaginertia="4.23401e-06 3.60599e-06 1.65e-06"/>
                <joint name="l_sho_pitch" axis="0 1 0" range="-90 90"/>
                <!-- <geom type="mesh" mesh="la1" class="visual"/> -->
                <geom type="mesh" mesh="la1c" class="collision"/>
                <body name="l_sho_roll_link" pos="0.019 0.0285 -0.01">
                    <inertial pos="-0.01844 0.04514 0.00028" quat="0.501853 0.50038 -0.498173 0.499588" mass="0.17758" diaginertia="0.000234742 0.00022804 3.04183e-05"/>
                    <joint name="l_sho_roll" axis="-1 0 0" range="0 45"/>
                    <!-- <geom type="mesh" mesh="la2" class="visual"/> -->
                    <geom type="mesh" mesh="la2c" class="collision"/>
                    <body name="l_el_link" pos="0 0.0904 -0.0001">
                        <inertial pos="-0.019 0.07033 0.0038" quat="0.483289 0.51617 -0.51617 0.483289" mass="0.04127" diaginertia="6.8785e-05 6.196e-05 1.2065e-05"/>
                        <joint name="l_el" axis="1 0 0" range="0 90"/>
                        <!-- <geom type="mesh" mesh="la3" class="visual"/> -->
                        <geom type="mesh" mesh="la3c" class="collision"/>
                    </body>
                </body>
            </body>


        </body>
    </worldbody>

</mujoco>

